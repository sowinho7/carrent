<script src="~/lib/jquery/dist/jquery.js"></script>
<link href="//netdna.bootstrapcdn.com/bootstrap/3.0.3/css/bootstrap.min.css" rel="stylesheet" id="bootstrap-css">


<meta name="viewport" content="width=device-width, initial-scale=1">
<link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">

@inject SignInManager<CarRental.BLL.Entities.User> SignInManager


@if (SignInManager.IsSignedIn(User))
{

    @using CarRental.BLL.Entities
    @model List<Cars>;

    <link rel="stylesheet" href="~/css/StyleSheet.css" />

    <div id="naglowek">
        <div class="col-md-12">
            <h2>Samochody</h2>
        </div>
    </div>
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div id="wiersze">
                    <table class="table table-striped">
                        <tr>
                            <th>Marka</th>
                            <th>Model</th>
                            <th>Opcje</th>
                        </tr>
                        @foreach (var itm in Model)
                        {
                            <tr>
                                <td>@itm.Brand</td>
                                <td>@itm.CarModel</td>
                                <td>
                                    @Html.ActionLink("Usuń", "DeleteCar", new { Id = itm.Id }, new { @class = "btn btn-danger" })
                                    @Html.ActionLink("Szczegóły", "GetCar", new { Id = itm.Id }, new { @class = "btn btn-warning" })
                                    @Html.ActionLink("Edytuj", "EditCar", new { Id = itm.Id }, new { @class = "btn btn-primary" })
                                </td>
                            </tr>
                        }
                    </table>
                </div>
            </div>
        </div>
    </div>
    <div class="col-md-12">
        @Html.ActionLink("Dodaj", "AddCar", (object)null, new { @class = "btn btn-primary" })
    </div>




}

<script src="//netdna.bootstrapcdn.com/bootstrap/3.0.3/js/bootstrap.min.js"></script>
<script src="//code.jquery.com/jquery-1.11.1.min.js"></script>

